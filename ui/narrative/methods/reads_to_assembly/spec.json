{   "ver": "1.0.0",

    "authors": [
        "janaka"
    ],
    "contact": "help@kbase.us",
    "visible": false,
    "categories": ["active","upload"],
    "widgets": {
        "input": null,
        "output": "kbaseReportView"
    },
    "parameters": [
        {
            "id" : "file_path_list",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "/data/bulktest/data/bulktest/janakakbase/reads/" ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types" : [ ]
            }
        },
        {
            "id" : "reads_id",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "" ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types" : ["KBaseAssembly.PairedEndLibrary"],
                "is_output_name":true

            }
        },
        {
            "id" : "insert_size",
            "optional" : true,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "300" ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types" : [ ]
            }
        },
        {
            "id" : "std_dev",
            "optional" : true,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "60" ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types" : [ ]
            }
        },
        {
            "id" : "reads_type",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "PairedEndLibrary" ],
            "field_type" : "text",
            "text_options" : {
                "valid_ws_types" : [ ]
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "genome_transform",
            "method": "reads_to_assembly",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace"
                },
                {
                    "input_parameter": "reads_type",
                    "target_property": "reads_type"
                },
                {
                    "input_parameter": "file_path_list",
                    "target_property": "file_path_list"
                },
                {
                    "input_parameter": "reads_id",
                    "target_property": "reads_id"
                },
                {
                    "input_parameter": "insert_size",
                    "target_property": "insert_size"
                },
                {
                    "input_parameter": "std_dev",
                    "target_property": "std_dev"
                }
            ],
            "output_mapping": [
                {
                  "narrative_system_variable": "workspace",
                  "target_property": "ws"
                },
                {
                  "constant_value": "KBaseAssembly.PairedEndLibrary",
                  "target_property": "type"
                },
                {
                    "input_parameter": "file_path_list",
                    "target_property": "id"
                },
                {
                    "constant_value": "16",
                    "target_property": "report_window_line_height"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
